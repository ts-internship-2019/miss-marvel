@model iWasHere.Domain.Models.DictionaryLandmarkPeriod

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddLandmarkPeriod</title>
</head>
<body>

<h4>DictionaryLandmarkPeriod</h4>
<hr />
<div class="row">
    <div class="col-md-6">
        <form asp-action="AddEditLandmarkperiod">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                
                    <input type="hidden" asp-for="LandmarkPeriodId" />
                    <label asp-for="LandmarkPeriodName" class="control-label" ></label>
                    <input asp-for="LandmarkPeriodName" class="form-control" />
                    <span asp-validation-for="LandmarkPeriodName" class="text-danger"  ></span>
                
               
            </div>
                 
            <div class="form-row">
                <div class="form-group col-md-6">
                    <input type="submit" value="Salveaza" class="btn btn-primary btn-block" id="Save"/>
                </div>
                <div class="form-group col-md-6">
                    <a asp-action="LandmarkPeriod" class="btn btn-secondary btn-block">Renunta</a>
                </div>
            </div>
               
                
        </form>
    </div>
</div>
</body>
</html>
<script>
    function redirect() {

          location.href = '@Url.Content("/Dictionary/LandmarkPeriod")';
    };

    function clearText()  
{
    document.getElementById('landPeriodlbl').value = "";
} 
</script>
@section Scripts
{ 
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");};
}
